{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "panels": [
    {
      "type": "timeseries",
      "title": "API Requests per Endpoint",
      "targets": [
        {
          "expr": "sum by (endpoint) (rate(iris_api_requests_total[1m]))",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Predictions Made",
      "targets": [
        {
          "expr": "rate(iris_api_predictions_total[1m])",
          "legendFormat": "Predictions/sec",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
    },
    {
      "type": "timeseries",
      "title": "Average Request Latency (seconds)",
      "targets": [
        {
          "expr": "sum(rate(iris_api_request_latency_seconds_sum[1m])) by (endpoint) / sum(rate(iris_api_request_latency_seconds_count[1m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 }
    },
    {
      "type": "stat",
      "title": "Model Loaded",
      "targets": [
        {
          "expr": "iris_api_model_loaded",
          "legendFormat": "{{model}}",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 }
    }
  ],
  "schemaVersion": 36,
  "style": "dark",
  "tags": ["iris", "mlops", "prometheus"],
  "templating": { "list": [] },
  "time": { "from": "now-5m", "to": "now" },
  "timezone": "",
  "title": "Iris MLOps",
  "version": 1
}
